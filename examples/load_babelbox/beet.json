{
  "pipeline": [
    {
      "pipeline": ["beet.contrib.babelbox"],
      "meta": {
        "babelbox": {
          "load": ["translations/*.csv"],
          "filename_prefix": true
        }
      }
    },
    {
      "pipeline": ["beet.contrib.babelbox"],
      "meta": {
        "babelbox": {
          "load": ["translations.csv"]
        }
      }
    },
    {
      "pipeline": ["beet.contrib.babelbox"],
      "meta": {
        "babelbox": {
          "load": ["translations.csv"],
          "namespace": "demo"
        }
      }
    },
    {
      "pipeline": ["beet.contrib.babelbox"],
      "meta": {
        "babelbox": {
          "load": ["unicode_escape.csv"],
          "unicode_escape": true
        }
      }
    }
  ]
}
